<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane
        alignment="CENTER"
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="controller.ChoiceController"
        prefHeight="400.0" prefWidth="600.0">

    <Button GridPane.columnIndex="0" GridPane.rowIndex="0" text="Service" onAction="#serviceEntry" />
    <Button GridPane.columnIndex="0" GridPane.rowIndex="1" text="Reports" onAction="#printReports" />
    <Button GridPane.columnIndex="0" GridPane.rowIndex="2" text="Edit Records" onAction="#editRecords" />

</GridPane>
